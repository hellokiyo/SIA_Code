package myjdbc;

public class LiteralTest {
	public static void main(String[] args) {
		System.out.println("Hello");
		//1, 1.0, 1.0f, '1', "1"
		// int, double, float, Character, String
		float myf = (float)1.0;
		double myf2 = 1.0;
		double myf3 = 1.0f;
		int num = '50';
		System.out.println(num);
	}

}


