package myjdbc;

public class CarTest {

	public static void main(String[] args) {
		Car car1 = new Car();
		car1.setName("Lightning Mcqueen");
		car1.setSpeed(300);

		Car car2 = new Car();
		car2.setName("Master");
		car2.setSpeed(80);
		
		Car car3 = new Car();
		car3.setName("Doc Hudson");
		car3.setSpeed(300);
		
		Car car4 = new Car();
		car4.setName("Sally Carrera");
		car4.setSpeed(200);
	}

}

class Car {
	String name;
	int speed;

	Car() {

	}

	void setName(String name) {
		this.name = name;
	}

	String setName() {
		return name;
	}
	
	void setSpeed(int speed) {
		this.speed = speed;
		System.out.println("차의 속력: " + speed);
	}

	int getSpeed() {
		return speed;
	}

}